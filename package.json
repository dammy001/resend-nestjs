{
  "name": "resend-nestjs",
  "version": "0.0.1",
  "description": "NestJS Package Template",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "private": false,
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typecheck:why": "tsc --noEmit --explainFiles > explainTypes.txt",
    "build": "pnpm typecheck && tsup src/index.ts --format cjs,esm --dts --clean --external @nestjs/common --external @nestjs/core",
    "watch": "pnpm run build -- --watch src",
    "dev": "pnpm -r --filter='./example/**' run start:dev",
    "lint": "eslint --cache . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "Template",
    "Package",
    "NestJs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Damilare Anjorin",
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": "^9 || ^10",
    "@nestjs/core": "^9 || ^10"
  },
  "devDependencies": {
    "@damilaredev/eslint-config-typescript": "^0.0.18",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.8",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.0",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "resend": "^0.17.0"
  }
}
